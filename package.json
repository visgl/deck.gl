{
  "name": "deck.gl",
  "description": "A WebGL overlay suite consists of multiple layers",
  "version": "1.0.2",
  "keywords": [
    "webgl",
    "visualization",
    "overlay",
    "layer",
    "babel",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-common/deck.gl.git"
  },
  "main": "src/index.js",
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify",
      "glslify",
      "envify"
    ]
  },
  "dependencies": {
    "earcut": "^2.0.6",
    "geojson-normalize": "0.0.1",
    "global": "^4.3.0",
    "lodash.flattendeep": "^3.0.2",
    "lodash.isequal": "^3.0.4",
    "lodash.isfunction": "^3.0.6",
    "lodash.throttle": "^3.0.4",
    "lodash.where": "^3.1.0",
    "philogl": "git+https://github.com/philogb/philogl.git",
    "viewport-mercator-project": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "^7.2.0",
    "d3-request": "^0.2.2",
    "envify": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.1",
    "faucet": "0.0.1",
    "glslify": "^5.0.0",
    "http-server": "^0.8.5",
    "immutable": "^3.7.5",
    "r-dom": "^2.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-map-gl": "^0.6.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "tape": "^4.0.0",
    "uber-standard": "^5.1.0",
    "watchify": "^3.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "scripts": {
    "lint": "uber-standard",
    "precommit": "npm run lint -s",
    "server": "./node_modules/http-server/bin/http-server -c-1 .",
    "start": "npm run watch & npm run server",
    "test": "babel-node test/index.js | faucet",
    "watch": "watchify -o example/bundle.js example/main.js -v"
  }
