    "@deck.gl/layers": "^8.8.0",
    "@deck.gl/mesh-layers": "^8.8.0",
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3 || ^4",
    "@loaders.gl/3d-tiles": "^3.2.5",
    "@loaders.gl/core": "^3.2.5",
    "@loaders.gl/csv": "^3.2.5",
    "@luma.gl/constants": "^8.5.16",
    "mapbox-gl": "^1.2.1"
  },
  "jupyterlab": {
