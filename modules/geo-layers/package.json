  "peerDependencies": {
    "@deck.gl/core": "^9.0.0-beta",
    "@deck.gl/extensions": "^9.0.0-beta",
    "@deck.gl/layers": "^9.0.0-beta",
    "@deck.gl/mesh-layers": "^9.0.0-beta",
    "@loaders.gl/core": "^4.2.0-alpha.5",
    "@luma.gl/core": "9.0.0-beta.10",
