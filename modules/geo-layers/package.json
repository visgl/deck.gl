{
  "name": "@deck.gl/geo-layers",
  "description": "deck.gl layers supporting geospatial use cases and GIS formats",
  "license": "MIT",
  "type": "module",
  "version": "9.0.0-beta.8",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl",
    "visualization",
    "geospatial",
    "layer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/deck.gl.git"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
