    "@loaders.gl/wms": "^4.2.0-alpha.5",
    "@luma.gl/gltf": "9.0.0-beta.10",
    "@luma.gl/shadertools": "9.0.0-beta.10",
    "@math.gl/core": "^4.0.0",
