{
  "description": "CARTO vector tileset declarative example",
  "initialViewState": {
    "latitude": 40.7368521,
    "longitude": -73.8536065,
    "zoom": 9,
    "pitch": 0,
    "bearing": 0
  },
  "views": [
    {
      "@@type": "MapView",
      "controller": true,
      "mapStyle": "https://basemaps.cartocdn.com/gl/positron-gl-style/style.json"
    }
  ],
  "layers": [
    {
      "@@type": "VectorTileLayer",
      "data": {
        "@@function": "vectorTilesetSource",
        "accessToken": "eyJhbGciOiJIUzI1NiJ9.eyJhIjoiYWNfbHFlM3p3Z3UiLCJqdGkiOiI0YTA5YjlmMSJ9.SQlcn7UucERdnq9O6ELm_Wi02dyRIJ_2KSTEnlXFjIc",
        "connectionName": "carto_dw",
        "tableName": "cartobq.public_account.nyc_taxi_points_demo_id"
      },
      "getFillColor": {
        "@@function": "colorBins",
        "attr": "aggregated_total",
        "domain": [10, 100, 1000, 10000, 100000, 1000000],
        "colors": "SunsetDark"
      },
      "pointRadiusMinPixels": 2,
      "stroked": false
    }
  ],
  "widgets": [
    {
      "@@type": "ZoomWidget"
    },
    {
      "@@type": "CompassWidget"
    }
  ]
}